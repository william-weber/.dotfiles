[user]
	name = william-weber
	email = taj.tek@gmail.com
[color]
	diff = auto
	status = auto
	branch = auto
[color "branch"]
    current = yellow reverse
    local = yellow
    remote = green
[color "diff"]
    plain = normal
    meta = green
    frag = cyan
    old = red
    new = green
    commit = yellow
    whitespace = normal red
[color "status"]
    added = yellow
    changed = green
    untracked = cyan
[alias]
    st = status
    ci = commit
    br = branch
    co = checkout
    df = diff
    lg = log -p
[apply]
    whitespace = nowarn

[push]
	default = simple

[hub]
protocol = https

[interactive]
  singlekey = true
[grep]
	extendRegexp = true
	lineNumber = true
