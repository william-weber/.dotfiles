[user]
	name = Sandy Weber
	email = sandyw@beardendesigns.com
[color]
	diff = auto
	status = auto
	branch = auto
[color "branch"]
    current = yellow reverse
    local = yellow
    remote = green
[color "diff"]
    meta = yellow bold
    frag = magenta bold
    old = red bold
    new = green bold
[color "status"]
    added = yellow
    changed = green
    untracked = cyan
[alias]
    st = status
    ci = commit
    br = branch
    co = checkout
    df = diff
    lg = log -p
[apply]
    whitespace = nowarn

